@model MusicHouse.ViewModels.GroupViewModel

    @foreach (var item in Model.Groups)
    {
        <div class="soundtheme-blogs">
            <div class="col-md-5">
                <div class="soundtheme-blog-list-img">
                    <div class="soundtheme-single-gallery">
                        <a href="http://pock.be/sound/normal-blog-post/">
                            <img width="450" height="450" src="http://pock.be/sound/wp-content/uploads/2015/10/photo-1436476549056-cdf7e8f3d8e3-450x450.jpg" class="attachment-soundtheme-thumb-medium size-soundtheme-thumb-medium wp-post-image" alt="" srcset="http://pock.be/sound/wp-content/uploads/2015/10/photo-1436476549056-cdf7e8f3d8e3-450x450.jpg 450w, http://pock.be/sound/wp-content/uploads/2015/10/photo-1436476549056-cdf7e8f3d8e3-150x150.jpg 150w, http://pock.be/sound/wp-content/uploads/2015/10/photo-1436476549056-cdf7e8f3d8e3-250x250.jpg 250w" sizes="(max-width: 450px) 100vw, 450px" />
                            <div class="soundtheme-hover-play">
                                <i class="fa fa-hand-peace-o"></i>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-md-7">
                <div class="soundtheme-blog-list">
                    <h1>
                        <a href="http://pock.be/sound/normal-blog-post/">@item.Data.GroupName</a>
                    </h1>
                    <hr />
                    <div class="soundtheme-post-detail">
                        <p>
                            A faucibus velit adipiscing est vitae scelerisque tristique
                            eu adipiscing porta suspendisse aliquet duis class phasellus
                            euismod fames parturient hac ac elit elit eleifend litora condimentum at.
                            Lorem parturient primis viverra nunc ipsum nisl eleifend donec consectetur
                            magna fusce hendrerit a lobortis laoreet adipiscing ad suspendisse dui a dictum.

                        </p>
                        @Html.ActionLink("Details", "Details", new { id = @item.Reference.Id })
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
    }


